{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "nexus-connect",
  "mainBinaryName": "nexus-connect",
  "version": "0.1.0",
  "identifier": "com.nexus-connect.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Nexus Connect",
        "width": 1200,
        "height": 700,
        "minWidth": 1200,
        "minHeight": 700
      }
    ]
  }
}
